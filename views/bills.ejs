    <!DOCTYPE html>
    <html lang="en">

    <head>
        <style>
            body {
                padding: 0;
                margin: 0;
                box-sizing: border-box;
                background-image: linear-gradient(to bottom, #0f0c29, #302b63, #24243e);


            }

            * {
                transition: all .2s ease-in-out;
            }





            .allBills {
                position: relative;
                width: 100%;
                background-color: #343a40 !important;
                z-index: 1;
                border-top: 5px solid blue;
                margin-top: 5px;

            }

            .divBillsAndImg {
                width: 100%;
                position: relative;
                display: flex;
                flex-direction: row;


            }

            .onlyDivBills {
                width: 50%;
                position: relative;
                display: flex;
                flex-direction: column;
                padding: 10px;
                font-size: 20px;

            }

            .electricity,
            .water,
            .phone,
            .net,
            .otherBills,
            .padget {
                width: 100%;
                height: 70px;
                position: relative;
                border-bottom: 1px solid white;
                border-right: 1px solid white;
                margin-top: 50px;
            }

            .electricity span,
            .water span,
            .phone span,
            .net span,
            .otherBills span,
            .padget span {
                color: white;
                position: absolute;
                margin-left: 10px;
                margin-top: 15px;
            }
            .padget span{
                font-size: 22px;
            }
            .padgeText{
                color:#fff
            }
            .padgeText:focus{
                border-bottom-color: #17224D;
                color:aqua
            }

            .onlyImg {
                width: 50%;
                position: relative;
                padding: 10px;

            }

            @media only screen and (max-width:850px) {
                .onlyImg {
                    display: none;
                }

                .onlyDivBills {
                    width: 90%;
                }
            }

            .contcat {
                width: 100%;
                position: relative;
                padding: 15px;
                background-repeat: no-repeat;
                flex-direction: row;
                justify-content: space-around;
                background-color: #17224D;
                border-radius: 2%;
                -webkit-perspective: 800;
                -webkit-perspective-origin: 50% 200px;
                -moz-perspective: 800;
                -moz-perspective-origin: 50% 200px;
            }

            .cubes {
                display: flex;
                width: 100%;
                position: relative;
            }

            .cube {
                position: relative;
                margin: 100px auto 0;
                height: 300px;
                width: 300px;
                -webkit-transition: -webkit-transform .5s linear;
                -webkit-transform-style: preserve-3d;

                -moz-transition: -moz-transform .5s linear;
                -moz-transform-style: preserve-3d;
            }

            .face {
                position: absolute;
                text-align: center;
                height: 200px;
                width: 300px;
                padding: 5px;
                font-size: 15px;
                line-height: 1em;
                color: #fff;
                border: 1px solid #555;
                border-radius: 3px;
                padding-top: 7px;
            }

            .cube .front {
                -webkit-transform: translateZ(150px);
                -moz-transform: translateZ(150px);
                background-color: #343a40;
            }

            .cube .side {
                -webkit-transform: rotateY(-90deg) translateZ(150px);
                -moz-transform: rotateY(-90deg) translateZ(150px);
                background-color: #555;
                line-height: 30px;
                padding: 5px;
            }

            .cube .side .connection {
                margin-top: 20px;
                width: 80%;

            }

            .cube:hover {
                -webkit-transform: rotateY(90deg);
            }

            .lastP {
                line-height: 20px;
            }

            @media(max-width:716px) {
                .cubes {
                    display: block;
                }
            }

            .contact_header {
                width: 100%;
                text-align: center;
                color: #fff;
                font-size: 25px;
                font-weight: bold;
                height: auto;
                position: absolute;
            }

            .about_website {
                max-width: 500px;
                color: whitesmoke;
                font-size: 20px;
                font-weight: 400;
                font-family: Arial, Helvetica, sans-serif;
                position: relative;
                left: 50%;
                padding: 1px;
                margin-left: -250px;
                margin-top: -50px;
            }

            .about_website:first-letter {
                font-size: 30px;

            }

            .about_website strong {
                font-size: 30px;
                color: red;
            }

            .user_name {
                font-size: 30px;
                font-family: Arial, Helvetica, sans-serif;
                color: white;
                margin-right: 30px;
                font-weight: 600;
            }

            .img_bill {
                color: dodgerblue;
            }

            .moverow {
                animation: rowanimation 1s infinite;
                transition: .5s all ease-in-out;
            }

            .moverow1 {
                animation: rowanimation1 1s infinite;
                transition: .5s all ease-in-out;
            }

            .moverow2 {
                animation: rowanimation2 1s infinite;
                transition: .5s all ease-in-out;
            }

            .moverow3 {
                animation: rowanimation3 1s infinite;
                transition: .5s all ease-in-out;
            }

            .moverow4 {
                animation: rowanimation4 1s infinite;
                transition: .5s all ease-in-out;
            }

            @keyframes rowanimation {
                0% {
                    transform: translateX(0);
                }

                50% {
                    transform: translateX(150%);
                }

                100% {
                    transform: translateX(0);

                }
            }

            @keyframes rowanimation1 {
                0% {
                    transform: translateX(0);
                }

                50% {
                    transform: translateX(150%);
                }

                100% {
                    transform: translateX(0);

                }
            }

            @keyframes rowanimation2 {
                0% {
                    transform: translateX(0);
                }

                50% {
                    transform: translateX(150%);
                }

                100% {
                    transform: translateX(0);

                }
            }

            @keyframes rowanimation3 {
                0% {
                    transform: translateX(0);
                }

                50% {
                    transform: translateX(150%);
                }

                100% {
                    transform: translateX(0);

                }
            }

            @keyframes rowanimation4 {
                0% {
                    transform: translateX(0);
                }

                50% {
                    transform: translateX(150%);
                }

                100% {
                    transform: translateX(0);

                }
            }

